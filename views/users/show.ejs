<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Username</h1>

    <%=user.username%>

    <section>
      <h2>Posts by <%=user.username%></h2>
        <ul>
          <% for(let i = 0; i < user.photos.length; i++) { %>
            <li><img src='<%=user.photos[i].img%>'></li>
          <% } %>

        </ul>
    </section>

  </body>
</html>
